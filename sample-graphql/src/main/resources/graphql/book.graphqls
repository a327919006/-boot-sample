#查询入口
extend type Query {
    findAllBooks: [Book]!
    countBooks: Int!
}

type Book {
    id: String!
    author: Author
    title: String!
    isbn: String!
    price(city:String!): Int
    pageCount: Int
    createTime: String
    updateTime: String
}

input BookInput {
    title: String!
    isbn: String!
    pageCount: Int
    authorId: Int
}

